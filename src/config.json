{
  "batch_size": 32,
  "images_per_class": 100,
  "validation_split": 0.2,
  "test_split": 0.2,
  "number_of_epochs": 30,
  "classes": [
    0,
    1,
    2
  ],
  "image_shape": [
    150,
    150,
    3
  ],
  "model": {
    "optimizer": [
      "adam"
    ],
    "loss": [
      "categorical_crossentropy"
    ],
    "metrics": [
      "accuracy"
    ],
    "dense_activation": ["relu"],
    "dropout_rate": [0.2],
    "dense_regularization": [0.01],
    "layers": [1, 2, 3, 4],
    "convolutional_base": [
      "Xception",
      "VGG16",
      "ResNet152V2",
      "EfficientNetB7"
    ]
  }
}